{
  "plugins": [
    [
      "babel-plugin-module-resolver",
      {
        "alias": {
          "db": "./src/db",
          "services": "./src/services",
          "utils": "./src/utils"
        }
      }
    ],
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime"
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ]
}
